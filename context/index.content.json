lector('i');\n\n      // Set initial state\n      gsap.set([title, description, icon], { opacity: 0, x: -30 });\n\n      ScrollTrigger.create({\n        trigger: card,\n        start: \"top 80%\",\n        end: \"top 20%\",\n        onEnter: () => {\n          gsap.to([icon, title, description], {\n            opacity: 1,\n            x: 0,\n            duration: 0.8,\n            stagger: 0.1,\n            ease: \"power2.out\"\n          });\n        }\n      });\n    });\n  </script>\n</body>\n</html> ",
